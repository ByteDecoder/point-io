<h2><%= @party.votes.count %> votes are in!</h2>
<table class="table table-striped table-hover">
  <% @party.votes.each do |vote| %>
    <tr>
      <td><%= vote.points %></td>
    </tr>
  <% end %>
</table>

<%= form_for @party do |f| %>
  <%= f.submit 'Restart!', class: 'btn btn-danger' %>
<% end %>

<script>
  setTimeout(function() {
      window.location.href = window.location.href;
   }, 1000);
</script>
